	<xs:complexType name="t_ValueAddedTax">
        <xs:annotation>
        	<xs:documentation>Typ für Mehrwertsteuersätze. Mehrwertsteuertsätze existieren pro Land und können für unterschiedliche Gültigkeitszeiträume, unterschiedliche Steuersätze haben.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="validity" type="t_Validity" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Liste der Gültigkeiten verschiedener Steuersätze.</xs:documentation>
				</xs:annotation></xs:element>
		</xs:sequence>
		<xs:attribute name="id" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation>Eindeutige Kennung des Mehrwertsteuersatzes.</xs:documentation>
			</xs:annotation></xs:attribute>
		<xs:attribute name="countrycode" use="required">
            <xs:annotation>
            	<xs:documentation>ISO-Alpha-2-Code des Landes, zu dem dieser Steuersatz gehört.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
				<xs:restriction base="xs:string">

					<xs:length value="2"></xs:length>
					<xs:pattern value="[A-Z]{2}"></xs:pattern>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="name" use="required">
            <xs:annotation>
            	<xs:documentation>Bezeichner des Steuersatzes.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="0"></xs:minLength>
					<xs:maxLength value="10"></xs:maxLength>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="t_Validity">
        <xs:annotation>
        	<xs:documentation>Typ für einen Mehrwertsteuersatz mit seinem Gültigkeitszeitraum.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="start" type="xs:date" use="required">
        	<xs:annotation>
        		<xs:documentation>Beginn der Gültigkeit dieses Steuersatzes.</xs:documentation>
        	</xs:annotation></xs:attribute>        <xs:attribute name="rate" use="required">        	<xs:annotation>        		<xs:documentation>        			Mehrwertsteuersatz in Prozent.        		</xs:documentation>        	</xs:annotation>        	<xs:simpleType>        		<xs:restriction base="xs:double">        			<xs:pattern value="\d{1,2}([.]\d{1,2})?"></xs:pattern>
        		</xs:restriction>
        	</xs:simpleType>
        </xs:attribute>
	</xs:complexType>

	<xs:complexType name="t_StorageUnit">

		<xs:annotation>
			<xs:documentation>
				Lagereinheit des Produkts. Falls keine Lagereinheit
				angegeben wird, wird bei der Produktanlage die
				Lagereinheit "Stück" genommen.

				Die Einheit wird entweder über die Bezeichnung in den
				Basisdaten oder die interne ID identifiziert. Eines von
				beiden muss angegeben werden.

				Fehlende oder mehrdeutige Bezeichnungen werden im
				Ereignisprotokoll festgehalten.
			</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="name" type="xs:string" maxOccurs="1"
				minOccurs="1">
