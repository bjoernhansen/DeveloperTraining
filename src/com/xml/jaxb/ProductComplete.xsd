					<xs:documentation>
						Wird bisher beim Import beruecksichtigt.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" type="xs:int" maxOccurs="1"
				minOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Die ID der Umrechnungseinheit. Bisher nur beim
						Export an PIM beruecksichtigt, nicht z.B. beim
						Import.
					</xs:documentation>
				</xs:annotation>
			</xs:element>

		</xs:choice>
		<xs:attribute name="isoCode" type="xs:string"></xs:attribute>
	</xs:complexType>

