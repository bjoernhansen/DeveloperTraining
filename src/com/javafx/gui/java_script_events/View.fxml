<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="255.0"
			prefWidth="408.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<fx:script>
		function letsGo(){
		label1.setText(tf1.getText());
		}
	</fx:script>
	<TextField fx:id="tf1" layoutX="126.0" layoutY="53.0"/>
	<Button layoutX="290.0" layoutY="53.0" mnemonicParsing="false"
		text="Button" onAction="letsGo()"/>
	<Label fx:id="label1" layoutX="126.0" layoutY="94.0" prefHeight="77.0"
		   prefWidth="149.0" text="Label"/>
</AnchorPane>
